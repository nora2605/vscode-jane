  "description": "Support for the Jane and JOHN Languages",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jane",
      "aliases": ["Jane", "jane"],
      "extensions": [".jane",".jn",".ðŸ©µ"],
      "configuration": "./jane-language-configuration.json",
      "icon": {
        "light": "./icons/janelogo.svg",
        "dark": "./icons/janelogo.svg"
      }
    }, {
      "id": "john",
      "aliases": ["John", "john"],
      "extensions": [".john",".ðŸ©·"],
      "configuration": "./john-language-configuration.json",
      "icon": {
        "light": "./icons/johnlogo.svg",
        "dark": "./icons/johnlogo.svg"
      }
    }],
    "grammars": [{
      "language": "jane",
      "scopeName": "source.jane",
